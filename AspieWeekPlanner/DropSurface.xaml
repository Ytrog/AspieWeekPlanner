<UserControl x:Class="AspieWeekPlanner.DropSurface"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AspieWeekPlanner"
             Name="DropSurfaceView"
             mc:Ignorable="d" 
             d:DesignHeight="150" d:DesignWidth="150">
    <Grid>
        <StackPanel Orientation="Vertical" AllowDrop="True" Drop="StackPanel_Drop" Background="White" Name="spStack" DragOver="SpStack_DragOver"></StackPanel>
    </Grid>
</UserControl>
